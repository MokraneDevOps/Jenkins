pipeline {
  agent any
  stages {
    stage('Message ') {
      parallel {
        stage('Message ') {
          steps {
            echo 'Hello World!'
          }
        }

        stage('Message') {
          steps {
            echo 'Bonjour'
          }
        }

      }
    }

    stage('message ') {
      steps {
        echo 'helle world!'
      }
    }

  }
}