pipeline {
  agent any
  stages {
    stage('Message ') {
      parallel {
        stage('Message ') {
          steps {
            echo 'Hello World!'
          }
        }

        stage('Message') {
          steps {
            echo 'Bonjour'
          }
        }

      }
    }

    stage('TEST') {
      steps {
        echo 'BONJOUR'
      }
    }

    stage('M') {
      steps {
        echo 'b'
      }
    }

  }
}